# 设置时区
TZ=Asia/Shanghai
# 设置网络模式
NETWORKS_DRIVER=bridge
# 宿主机host
LOCAL_HOST=192.168.200.135
LOCAL=localhost

# Golang 配置########
GO_SERVER_PORT=8888

# PATHS ##########################################
# 宿主机上代码存放的目录路径
CODE_PATH_HOST=../../../z-common
# 宿主机上Mysql Reids数据存放的目录路径
DATA_PATH_HOST=./data
# 宿主机上Mysql 初始化文件
DATA_INIT_PATH=./mysql/sql_init

# Redis 配置########
REDIS_PORT=6379
REDIS_BACKUP_PORT=6479


# Mysql 配置########
DB_CONTAINER_NAME=mysqlDB
DB_CHARACTER=utf8
MYSQL_PORT=3306
#MYSQL_USERNAME=admin
#MYSQL_PASSWORD=123456
MYSQL_ROOT_PASSWORD=123756
TARGET_DATABASE_NAME=common

# PhpAdminMysqlManage 配置########
# Mysql 可视化管理用户名称，同 MYSQL_USERNAME
MYSQL_MANAGE_USERNAME=admin
# Mysql 可视化管理用户密码，同 MYSQL_PASSWORD
MYSQL_MANAGE_PASSWORD=123456
# Mysql 可视化管理ROOT用户密码，同 MYSQL_ROOT_PASSWORD
MYSQL_MANAGE_ROOT_PASSWORD=123756
# Mysql 服务地址
MYSQL_MANAGE_CONNECT_HOST=mysql
# Mysql 服务端口号
MYSQL_MANAGE_CONNECT_PORT=3306
# Mysql 可视化管理映射宿主机端口号，可在宿主机127.0.0.1:1000访问
MYSQL_MANAGE_PORT=1000

# Apollo 配置########
APOLLO_CONFIG_SERVICE_PORT=8080
APOLLO_ADMIN_SERVICE_PORT=8090
APOLLO_PORTAL_PORT=8070

# ETCD 配置########
ETCD_CLIENT_PORT=2379
ETCD_PEER_PORT=2380

# JAEGER 配置########
JAEGER_PORT=5000
JAEGER_DATA_PORT=6831

# PROMETHEUS 配置########
PROMETHEUS_PORT=9090

# ROCKETMQ 配置########
ROCKET_NAME_SERVICE_PORT=9876
ROCKET_BROKER_PORT1=10909
ROCKET_BROKER_PORT2=10911
ROCKET_CONSOLE_PORT=18080

# RABBITMQ 配置########
RABBIT_SERVICE_PORT1=5672
RABBIT_SERVICE_PORT2=15672

# NATS 配置#######
NATS_CONF_URL=./nats/conf/nats-server.conf